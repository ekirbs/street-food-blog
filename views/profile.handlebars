<figure>
  <img src="/images/nyc-panorama.jpg" alt="A panorama image of New York City."/>
</figure>
    
<p>This is the profile page.</p>

{{#each posts as |post| }}
<div class="row mb-4 project">
  <div class="col-md-5">
    <h2>
      {{{get_img}}}
      <a href="/post/{{post.id}}">{{post.title}}</a>
    </h2>
    <p>Created by {{post.user_id}}</p>
  </div>
  <div class="col-md-7">
    <p>
      {{post.post_body}}
    </p>
  </div>
</div>
{{/each}}