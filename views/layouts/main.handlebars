<body>
  <div id="navbar">
    <header class="display-flex justify-space-between align-center p-2" id="anchor-home">
      <a href="/"><h1>Street Cuisine</h1></a>
      <img src="/images/streetfood-icon.png" alt="An image of street food." />
      <p class="lead">
        Your one-stop-shop for fine dining on the streets.
      </p>
      <p id="currentDay" class="lead"></p>
    </header>

    <nav>
      <a href="/weather">5-Day Weather Forecast</a>
      {{#if logged_in}}
      <a href="/profile">Profile</a>
      <button id="logout" class="btn-no-style">Logout</button>
      {{else}}
      <a class="float-right" href="/login">Login</a>
      {{!-- <a class="float-right" href="/auth/login">Login</a> --}}
      {{/if}}
    </nav>
  </div>

  <main class="container container-fluid mt-5">
    <div class="posts">
      {{{body}}}
    </div>
          <section class="section-label contact-park-header">
        <h3>Contact the Creators</h3>
      </section>

      <section class="Contact-Info">
        <!-- <a href="tel:5558675309">Jenny: (555) 867-5309</a> -->
        <a href="https://www.youtube.com/watch?v=dQw4w9WgXcQ">Customer Service</a>
        <a href="https://github.com/ekirbs/street-food-blog"><img src="/images/streetfood-icon.png" id="foodcart" alt="foodcart" />Boston Street Food blog</a>
        <div id="contact-park"></div>
      </section>
  </main>

  {{#if logged_in}}
  <script src="/js/logout.js"></script>
